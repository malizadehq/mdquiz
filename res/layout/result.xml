<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/world"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:padding="10dip" android:background="@drawable/result_bg"
	xmlns:android="http://schemas.android.com/apk/res/android" xmlns:myapp="http://schemas.android.com/apk/res/g.qmq">
	<LinearLayout android:id="@+id/resultBox"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:layout_alignParentTop="true" android:orientation="vertical">
		<TextView android:id="@+id/result_title"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:text="Quiz Result" android:textSize="40sp" android:textColor="#000000"
			android:layout_marginTop="10px" android:layout_marginBottom="20px"></TextView>
		<!-- <RelativeLayout android:id="@+id/staff" -->
		<!-- android:layout_width="fill_parent" android:layout_height="50px" -->
		<!-- android:layout_marginTop="40dip" android:background="@drawable/result_staff" -->
		<!-- android:paddingLeft="28px"> -->
		<!-- <TextView android:id="@+id/tvl" android:layout_width="wrap_content" -->
		<!-- android:layout_height="wrap_content" android:layout_alignParentLeft="true" -->
		<!-- android:layout_centerVertical="true" android:textSize="28sp" -->
		<!-- android:textColor="#FFFFFF" android:text="Total Time: 230 S"> -->
		<!-- </TextView> -->
		<!-- </RelativeLayout> -->
	</LinearLayout>
	<LinearLayout android:id="@+id/endBox"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:layout_below="@+id/resultBox" android:orientation="vertical"
		android:layout_marginTop="30px" android:gravity="right">
		<TextView android:id="@+id/ToMain" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Back To Main"
			android:textColor="#000000" android:textSize="30sp"
			android:layout_marginBottom="20px"></TextView>
		<TextView android:id="@+id/ToBoard" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Back To Board"
			android:textColor="#000000" android:textSize="30sp"></TextView>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_alignParentBottom="true"
		android:layout_below="@+id/endBox">
		<com.admob.android.ads.AdView android:id="@+id/ad"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			myapp:keywords="music" myapp:backgroundColor="#000000"
			myapp:primaryTextColor="#FFFFFF" myapp:secondaryTextColor="#CCCCCC">
		</com.admob.android.ads.AdView>
	</LinearLayout>
</RelativeLayout>
